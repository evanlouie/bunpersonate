{
  "name": "bunpersonate",
  "version": "0.1.0",
  "description": "Bun FFI bindings for curl-impersonate - make HTTP requests that impersonate real browsers",
  "module": "index.ts",
  "type": "module",
  "keywords": [
    "bun",
    "curl",
    "curl-impersonate",
    "ffi",
    "http",
    "browser",
    "impersonate",
    "scraping",
    "tls",
    "fingerprint"
  ],
  "license": "MIT",
  "author": "bunpersonate contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/bunpersonate.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/bunpersonate/issues"
  },
  "homepage": "https://github.com/yourusername/bunpersonate#readme",
  "scripts": {
    "typecheck": "bunx -p @typescript/native-preview tsc --noEmit",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "test:all": "bun test && bun run test/smoke.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prettier": "^3.6.2",
    "typescript": "^5"
  }
}
